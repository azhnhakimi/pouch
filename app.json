{
	"expo": {
		"name": "pouch",
		"slug": "pouch",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/customIcon.png",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/customSplash.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": false
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/customAdaptiveIcon.png",
				"backgroundColor": "#000000"
			}
		},
		"androidStatusBar": {
			"backgroundColor": "#000000",
			"barStyle": "light-content",
			"translucent": true,
			"hidden": false
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
